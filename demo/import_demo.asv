% [global_struct, packets, names] = json_reader('data2.json', true);


x = packets(:,1);
y = packets(:,2);
z = packets(:,3);
scatter3(x,y,z, '.b')

if min(x) >= 0
    xlim([0 max(x)])
end

if min(y) >= 0
    ylim([0, max(y)])
end

if min(z) >= 0
    zlim([0, max(z)])
end

get_data_by_name(packets, names, 'Coordinates');